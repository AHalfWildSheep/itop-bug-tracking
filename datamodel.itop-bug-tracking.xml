        <method id="OnInsert">
          <static>false</static>
          <access>protected </access>
          <code><![CDATA[    protected function OnInsert()
	{
        parent::OnInsert();
        $currentDate = time();
		$this->SetIfNull('update_date', $currentDate);
		$this->SetIfNull('creation_date', $currentDate);
	}]]></code>
        </method>
        <method id="OnUpdate">
          <static>false</static>
